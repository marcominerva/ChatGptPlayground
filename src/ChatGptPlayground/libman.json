{
    "version": "1.0",
    "defaultProvider": "cdnjs",
    "libraries": [
        {
            "library": "twitter-bootstrap@5.3.2",
            "destination": "wwwroot/lib/bootstrap"
        },
        {
            "library": "bootstrap-icons@1.11.2",
            "destination": "wwwroot/lib/bootstrap-icons"
        },
        {
            "library": "jquery@3.7.1",
            "destination": "wwwroot/lib/jquery"
        },
        {
            "library": "alpinejs@3.13.3",
            "destination": "wwwroot/lib/alpinejs"
        },
        {
            "provider": "jsdelivr",
            "library": "@alpinejs/persist@3.13.3",
            "destination": "wwwroot/lib/alpinejs/persist"
        },
        {
            "library": "marked@11.1.0",
            "destination": "wwwroot/lib/marked"
        }
    ]
}